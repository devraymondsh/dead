{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++ build active file",
      "command": "/usr/bin/cmake",
      "args": ["../c", "&&", "make"],
      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
